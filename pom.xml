    <module>plugins/mil.jpeojtrs.sca.dpd.edit</module>
    <module>plugins/mil.jpeojtrs.sca.partitioning</module>
    <module>plugins/mil.jpeojtrs.sca.partitioning.edit</module>
    <module>plugins/mil.jpeojtrs.sca.prf</module>
    <module>plugins/mil.jpeojtrs.sca.prf.edit</module>
    <module>plugins/mil.jpeojtrs.sca.profile</module>
    <module>plugins/mil.jpeojtrs.sca.profile.edit</module>
    <module>plugins/mil.jpeojtrs.sca.sad</module>
    <module>plugins/mil.jpeojtrs.sca.sad.edit</module>
    <module>plugins/mil.jpeojtrs.sca.scd</module>
    <module>plugins/mil.jpeojtrs.sca.scd.edit</module>
    <module>plugins/mil.jpeojtrs.sca.spd</module>
    <module>plugins/mil.jpeojtrs.sca.spd.edit</module>
    <module>plugins/mil.jpeojtrs.sca.util</module>
    <module>plugins/mil.jpeojtrs.sca.validator</module>
    <module>tests/mil.jpeojtrs.sca.dcd.tests</module>
    <module>tests/mil.jpeojtrs.sca.dmd.tests</module>
    <module>tests/mil.jpeojtrs.sca.dpd.tests</module>
    <module>tests/mil.jpeojtrs.sca.partitioning.tests</module>
    <module>tests/mil.jpeojtrs.sca.prf.tests</module>
    <module>tests/mil.jpeojtrs.sca.profile.tests</module>
    <module>tests/mil.jpeojtrs.sca.sad.tests</module>
    <module>tests/mil.jpeojtrs.sca.scd.tests</module>
    <module>tests/mil.jpeojtrs.sca.spd.tests</module>
    <module>tests/mil.jpeojtrs.sca.util.tests</module>
    <module>releng/p2</module>
  </modules>

  <repositories>
    <repository>
      <id>eclipse</id>
      <layout>p2</layout>
      <url>${eclipse}</url>
    </repository>

    <repository>
      <id>jacorb</id>
      <layout>p2</layout>
      <url>${jacorb}</url>
    </repository>
  </repositories>

</project>
